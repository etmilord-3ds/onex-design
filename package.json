{
  "name": "@mdsol/onex-design",
  "version": "0.1.1",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdsol/onex-design.git"
  },
  "main": "./dist/onex.umd.js",
  "module": "./dist/onex.es.js",
  "exports": {
    ".": {
      "import": "./dist/onex.es.js",
      "require": "./dist/onex.umd.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.2.0"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src --fix",
    "bs-css": "node src/tools/bs-css.js",
    "build": "vite build",
    "build:sb": "build-storybook",
    "build:sb-browsers": "node src/tools/sb-browsers.js",
    "build:sb-icons": "node src/tools/sb-icons.js",
    "build:theme": "sass ./src/scss/theme.scss dist/theme.css",
    "deploy": "yarn run lint && yarn run build && yarn run build:theme && npm publish",
    "deploy:sb": "storybook-to-ghpages"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.2.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/ui": "6.3.12",
    "@types/babel__core": "^7.1.18",
    "@types/bootstrap": "^5.1.9",
    "@types/mdx-js__react": "^1.5.5",
    "@types/prop-types": "^15.7.4",
    "@vitejs/plugin-react": "^1.1.4",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.19.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "got": "^12.0.1",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-scss": "^3.0.0",
    "storybook-builder-vite": "^0.1.13",
    "vite": "^2.7.13"
  },
  "packageManager": "yarn@3.2.0-rc.12"
}
