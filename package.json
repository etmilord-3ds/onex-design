{
  "name": "@mdsol/onex-design",
  "version": "0.1.5",
  "private": true,
  "author": "JT <jathompson@mdsol.com>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdsol/onex-design.git"
  },
  "main": "./dist/onex.umd.js",
  "module": "./dist/onex.es.js",
  "exports": {
    ".": {
      "import": "./dist/onex.es.js",
      "require": "./dist/onex.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/theme.css": {
      "import": "./dist/theme.css",
      "require": "./dist/theme.css"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.2.0"
  },
  "packageManager": "yarn@3.2.0-rc.12",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "rollup -c",
    "build:theme": "sass ./src/scss/theme.scss dist/theme.css",
    "build:sb-browsers": "node src/tools/sb-browsers.js",
    "build:sb-icons": "node src/tools/sb-icons.js",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src --fix",
    "bs-css": "node src/tools/bs-css.js",
    "storybook": "start-storybook -p 6006",
    "deploy:sb": "storybook-to-ghpages",
    "update:bl": "browserslist@latest --update-db"
  },
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@mdx-js/react": "^1.6.22",
    "@popperjs/core": "^2.11.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/ui": "6.3.12",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "browserslist": "^4.19.1",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.21.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.3",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^18.1.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "query-ast": "^1.0.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.2",
    "react-select": "^5.2.2",
    "rollup": "^2.66.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "sass-to-string": "^1.6.0",
    "scss-parser": "^1.0.5",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  }
}
